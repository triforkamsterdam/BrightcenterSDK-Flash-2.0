<?xml version="1.0" encoding="utf-8"?>

<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="21" minHeight="21"
             alpha.disabled="0.6">

    <!-- host component -->
    <fx:Metadata>
        <![CDATA[
        /**
         * @copy spark.skins.spark.ApplicationSkin#hostComponent
         */
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>

    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>

    <!-- outer circle white -->
    <s:Rect height="240" width="240" radiusX="120" radiusY="120">
        <s:fill>
            <s:SolidColor color="#FFFFFF"/>
        </s:fill>
    </s:Rect>

    <s:Group height="80" width="80" top="35" left="35">

        <!--circle1 orange-->
        <s:Rect height="80" width="80" radiusX="40" radiusY="40"
                top="20" left="20">
            <s:fill>
                <s:SolidColor color="#F47E2B"/>
            </s:fill>
        </s:Rect>

        <!--circle 2 white-->
        <s:Rect height="60" width="60" radiusX="30" radiusY="30"
                top="30" left="30">
            <s:fill>
                <s:SolidColor color="#FFFFFF"/>
            </s:fill>
        </s:Rect>

        <!-- circle 3 orange-->
        <s:Rect height="40" width="40" radiusX="20" radiusY="20"
                top="40" left="40">
            <s:fill>
                <s:SolidColor color="#F47E2B"/>
            </s:fill>
        </s:Rect>

        <!--circle 4 white-->
        <s:Rect height="20" width="20" radiusX="10" radiusY="10"
                top="50" left="50">
            <s:fill>
                <s:SolidColor color="#FFFFFF"/>
            </s:fill>
        </s:Rect>

    </s:Group>






</s:SparkSkin>